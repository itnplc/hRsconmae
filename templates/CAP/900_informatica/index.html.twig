{% extends 'CAP/master.html.twig' %}


{% block body %}
<table class="table">
	<thead>
		<tr>
			<th scope "col">Cont_Id</th>
			<th scope "col">Descripción</th>
			<th scope "col">Valor</th>
			<th scope "col">Fecha Modificación</th>
		</tr>
	</thead>
	<tbody>
		{% for consta in constants %}
		<tr>	
			    <td>{{ consta.getContId() }}</td>
			    <td>{{ consta.getDescription() }}</td>
			    <td>{{ consta.getValue() }}</td>
			    <td>{{ consta.getModificationDate() }}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
{% endblock %}	